{
  "name": "TabIndicator",
  "description": "",
  "props": {
    "className": {
      "type": "string | (state) => string",
      "description": "Class names applied to the element or a function that returns them based on the component's state."
    },
    "render": {
      "type": "React.ReactElement | (props, state) => React.ReactElement",
      "description": "A function to customize rendering of the component."
    },
    "renderBeforeHydration": {
      "type": "boolean",
      "default": "false",
      "description": "If `true`, the indicator will include code to render itself before React hydrates.\nThis will minimize the time the indicator is not visible after the SSR-generated content is downloaded."
    }
  },
  "cssVariables": {
    "--active-tab-bottom": {
      "description": "The active tab's bottom distance.",
      "type": "number"
    },
    "--active-tab-height": {
      "description": "The active tab's height.",
      "type": "number"
    },
    "--active-tab-left": {
      "description": "The active tab's left distance.",
      "type": "number"
    },
    "--active-tab-right": {
      "description": "The active tab's right distance.",
      "type": "number"
    },
    "--active-tab-top": {
      "description": "The active tab's top distance.",
      "type": "number"
    },
    "--active-tab-width": {
      "description": "The active tab's width.",
      "type": "number"
    }
  }
}
